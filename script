function oyunhilemenu()
MN5=gg.choice({
 '🏃  ⫸ Orta Hız[🎮]',
 '🐎  ⫸ Flash Hız[🎮]',
 ' 👁️‍🗨️ ⫸ ESP Player[🎮]',
 '🎐 Paraşüt[🎮]',
 '🔙 Geri'
   },nil,"😈[ABG] - Oyun Menüsü 🇹🇷")
 if MN5 == nil then 
 else
if MN5 == 1 then flareanten() end
if MN5 == 2 then flash() end
if MN5 == 3 then espmenu() end
if MN5 == 4 then parasut() end
if MN5 == 5 then HOME() end
end
PUBGMH = -1
end
